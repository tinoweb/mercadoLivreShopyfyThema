  <script>
        function checkCEPValue() {
            const cepInput = document.getElementById("CEP").textContent;

            if (cepInput.length === 8 || cepInput.length === 9) {
                fetch(`https://viacep.com.br/ws/${cepInput}/json/`)
                    .then(response => response.json())
                    .then(data => {
                        let prazoEntrega = '';
                        switch (data.uf) {
                            case 'AC':
                            case 'AP':
                            case 'AM':
                            case 'PA':
                            case 'RO':
                            case 'RR':
                            case 'TO':
                            case 'AL':
                            case 'BA':
                            case 'CE':
                            case 'MA':
                            case 'PB':
                            case 'PE':
                            case 'PI':
                            case 'RN':
                            case 'SE':
                                prazoEntrega = '3';
                                break;
                            case 'ES':
                            case 'MG':
                            case 'RJ':
                            case 'SP':
                            case 'PR':
                            case 'RS':
                            case 'SC':
                                prazoEntrega = '3';
                                break;
                            case 'DF':
                            case 'GO':
                            case 'MS':
                            case 'MT':
                                prazoEntrega = '3';
                                break;
                            default:
                                prazoEntrega = 'Prazo não definido';
                        }

                        const dataEntrega1 = new Date();
                        dataEntrega1.setDate(dataEntrega1.getDate() + parseInt(prazoEntrega));
                        const diaSemana1 = dataEntrega1.toLocaleString('pt-BR', { weekday: 'long' });
                        const dia1 = dataEntrega1.getDate();
                        const mes1 = dataEntrega1.toLocaleString('pt-BR', { month: 'long' });

                        document.getElementById("tempoentrega11").textContent = `${diaSemana1} ${dia1} de ${mes1}`;

                        localStorage.setItem("deliveryTime", `${diaSemana1} ${dia1} de ${mes1}`);
                        document.getElementById("cepmaster").textContent = localStorage.getItem("deliveryTime");
                    })
                    .catch(error => {
                        console.error('An error occurred while fetching CEP information.');
                    });
            }
        }

        // Check CEP value continuously
        setInterval(checkCEPValue, 1000); // Check every 1 second
    </script>

